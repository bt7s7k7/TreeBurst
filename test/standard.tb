// Test: advanced assignment short-circuit
$a = false
a ||= (increment(), true)
assertEqual(a, true)
assertEqual(getCounter(), 1)

a ||= (increment(), true)
assertEqual(a, true)
assertEqual(getCounter(), 1)

$b = void
b else= (increment(), 10)
assertEqual(b, 10)
assertEqual(getCounter(), 2)

