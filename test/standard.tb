// Test: array clone
$x = [1, 2, 3]
$y = x.clone()

assertEqual(x.length, y.length)
assertEqual(x[0], y[0])
assertEqual(x[1], y[1])
assertEqual(x[2], y[2])

// Test: array clear
$x = [1, 2, 3]
x.clear()
assertEqual(x.length, 0)

// Test: array slice
$x = [0, 1, 2, 3]
$y

y = x.slice(2)
assertEqual(y.length, 2)
assertEqual(y[0], 2)
assertEqual(y[1], 3)

y = x.slice(1, 3)
assertEqual(y.length, 2)
assertEqual(y[0], 1)
assertEqual(y[1], 2)

y = x.slice(-2)
assertEqual(y.length, 2)
assertEqual(y[0], 2)
assertEqual(y[1], 3)

y = x.slice(0, -2)
assertEqual(y.length, 2)
assertEqual(y[0], 0)
assertEqual(y[1], 1)

y = x.slice(-2, -1)
assertEqual(y.length, 1)
assertEqual(y[0], 2)

// Test: array splice
$x = [1, 2, 3]
$y

y = x.clone()
y.splice(1, 1)
assertEqual(y.length, 2)
assertEqual(y[0], 1)
assertEqual(y[1], 3)

y = x.clone()
y.splice(1, 1, [5, 6])
assertEqual(y.length, 4)
assertEqual(y[0], 1)
assertEqual(y[1], 5)
assertEqual(y[2], 6)
assertEqual(y[3], 3)

y = x.clone()
y.splice(y.length, 0, [4])
assertEqual(y.length, 4)
assertEqual(y[3], 4)

